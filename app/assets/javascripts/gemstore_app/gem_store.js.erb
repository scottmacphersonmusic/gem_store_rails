(function() {
  var app = angular.module('gemStore', ['store-directives', 'review-controller']);

  app.controller('StoreController', [ '$http', function($http){
    var store = this;
    store.products = [];
    $http.get("<%= asset_path 'gemstore_app/store-products.json' %>").success(function(data){
      store.products = data;
    });
  }]);
})();
